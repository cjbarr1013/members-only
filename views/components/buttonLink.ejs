<!-- prettier-ignore -->
<% const linkStyles = { 
  primary: `px-5 py-2.5 text-sm text-white font-medium rounded-lg bg-blue-600 
    focus:ring-4 focus:ring-blue-300 focus:outline-none hover:bg-blue-700`, 
  secondary: `py-2.5 px-5 text-sm text-gray-900 font-medium rounded-lg border 
    border-gray-200 bg-white focus:outline-none focus:z-10 focus:ring-4 
    focus:ring-gray-100 focus:z-10 focus:ring-4 focus:ring-gray-100 
    hover:bg-gray-100 hover:text-blue-700 dark:focus:ring-gray-700 
    dark:bg-gray-800 dark:text-gray-400 dark:border-gray-600 
    dark:hover:text-white dark:hover:bg-gray-700`, 
  } %>

<a href="<%= href %>" class="<%= linkStyles[type] %>"><%= label %></a>
